l = [5, 8, 1, 3, 7, 95, 41, 254, 547]
 # Simple method
t= sorted(l)
l.sort()
print(l)
print(t)

# Insertion sort with a for loop
for i in range(1, len(l)):
    key = l[i]
    j = i - 1
    # Move elements of l[0..i-1] that are greater than key to one position ahead
    while j >= 0 and l[j] > key:
        l[j + 1] = l[j]
        j -= 1
    l[j + 1] = key

print(l)
